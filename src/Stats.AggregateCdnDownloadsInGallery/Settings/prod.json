{
  "Initialization": {
    "BatchSize": 5000,
    "BatchSleepSeconds": 10
  },

  "GalleryDb": {
    "ConnectionString": "Data Source=tcp:#{Deployment.Azure.Sql.GalleryDatabaseAddress};Initial Catalog=NuGetGallery;Persist Security Info=False;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False;Application Name=Stats.AggregateCdnDownloads;AadTenant=#{Deployment.Azure.ActiveDirectory.Tenant};AadClientId=#{Deployment.Azure.ActiveDirectory.GalleryDbWriter.ClientId};AadCertificate=$$prod-gallerydb-writer$$"
  },

  "StatisticsDb": {
    "ConnectionString": "Data Source=tcp:#{Deployment.Azure.Sql.StatisticsDatabaseAddress};Initial Catalog=nuget-prod-statistics;Persist Security Info=False;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False;Application Name=Stats.AggregateCdnDownloads;AadTenant=#{Deployment.Azure.ActiveDirectory.Tenant};AadClientId=#{Deployment.Azure.ActiveDirectory.StatisticsDbWriter.ClientId};AadCertificate=$$prod-statisticsdb-writer$$"
  },

  "KeyVault_VaultName": "#{Deployment.Azure.KeyVault.VaultName}",
  "KeyVault_ClientId": "#{Deployment.Azure.KeyVault.ClientId}",
  "KeyVault_CertificateThumbprint": "#{Deployment.Azure.KeyVault.CertificateThumbprint}",
  "KeyVault_ValidateCertificate": true,
  "KeyVault_StoreName": "My",
  "KeyVault_StoreLocation": "LocalMachine"
}